                <div class="footer-inner">
                    <!-- Add your copyright text here -->
                    <div class="footer-text">
                        <%- themeConfig.footerInfo %> <span id="busuanzi_container_site_pv" style='display:none'>总访问量<span id="busuanzi_value_site_pv"></span>次</span>
                    </div>
                    <div class="go-up">
